.logo
  display: block
  float: left
  margin-left: 20px
  padding-top: 12px
  padding-left: 20px
  width: 250px
  height: $header-height
  background: $red-alfa
  border-bottom-left-radius: 4px
  border-bottom-right-radius: 4px
  -moz-border-radius-bottomleft: 4px
  -moz-border-radius-bottomright: 4px
  box-sizing: border-box

.logo__img
  display: block
  width: 210px
  height: 65px
  background: purple

